@inherits Custom.Hybrid.Razor14
@using ToSic.Razor.Blade;
@* --- This is the Bootstrap5 Edition of the File --- *@

<div class="app-filelist mb-4" @Kit.Toolbar.Default(Header)>
  @Html.Partial("Shared Part Title.cshtml", new { HeadingType = Header.HeadingType, Title = Header.Title })
  
  <div class="list-group">
    @foreach (var fileEntity in AsList(Data)) {      
      @Html.Partial("Shared Part File.cshtml", new { FileEntity = fileEntity } )
    }
  </div>
</div>

@Html.Partial("Assets.cshtml")
